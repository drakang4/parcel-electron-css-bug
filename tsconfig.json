{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
    ],
    "sourceMap": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "jsx": "react",
    "pretty": true,
    "rootDir": ".",
    "outDir": "build"
  },
  "exclude": [
    "node_modules"
  ]
}